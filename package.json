{
  "name": "super-orm",
  "version": "0.0.13",
  "description": "基于 Node.js 的 MySQL ORM 模块，支持缓存、多 MySQL 连接",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsc -p src",
    "test": "npm run compile && export DEBUG='super-orm:*' && export TS_NODE_PROJECT=test && mocha -t 10000 --require ts-node/register --require source-map-support/register test/**.ts",
    "test-cov": "npm run compile && export TS_NODE_PROJECT=test && istanbul cover _mocha -- -t 10000 --require ts-node/register test/**.ts && npm run test-cov-remap",
    "test-cov-remap": "remap-istanbul -i coverage/coverage.json  -t html -o coverage/html-report",
    "prepublish": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SuperID/super-orm.git"
  },
  "keywords": [
    "ORM",
    "MySQL"
  ],
  "author": "Zongmin Lei <leizongmin@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SuperID/super-orm/issues"
  },
  "homepage": "https://github.com/SuperID/super-orm#readme",
  "dependencies": {
    "@types/debug": "0.0.29",
    "@types/ioredis": "0.0.21",
    "@types/mysql": "0.0.30",
    "@types/node": "^6.0.48",
    "debug": "^2.3.2",
    "ioredis": "^2.4.0",
    "lei-coroutine": "^1.6.3",
    "lei-utils": "^3.1.0",
    "mysql": "^2.12.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.33",
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "remap-istanbul": "^0.7.0",
    "source-map-support": "^0.4.6",
    "ts-node": "^1.7.0",
    "tslint": "^4.0.2",
    "typescript": "^2.0.10"
  }
}
